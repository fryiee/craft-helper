{
  "name": "fryiee/craft-helper",
  "description": "A composer package to make scaffolding craft fields in migrations less painful.",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "matrix",
    "super table"
  ],
  "support": {
    "email": "hi@craigberry.co",
    "issues": "https://github.com/fryiee/craft-helper/issues?state=open",
    "source": "https://github.com/fryiee/craft-helper",
    "docs": "https://github.com/fryiee/craft-helper"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Craig Berry",
      "homepage": "http://craigberry.co"
    },
    {
      "name": "Red Tally",
      "homepage": "https://redtally.com.au"
    }
  ],
  "require": {
    "craftcms/cms": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Fryiee\\CraftHelper\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}
